/* china_ghphpt/clerkList/clerkList.wxss */

page {
    background: #f2f2f2;
}

.container {
    background: #f2f2f2;
}

.addBtn {
    width: 94%;
    margin: 20rpx auto;
    background: #fff url('data:image/png;base64,') no-repeat top 20rpx center;
    background-size: 30rpx auto;
    border-radius: 20rpx;
    height: 120rpx;
    text-align: center;
    line-height: 160rpx;
    font-size: 24rpx;
    color: #666;
}

.clerkList {
    width: 94%;
    margin: 20rpx auto;
    background: #fff;
    border-radius: 20rpx;
    padding: 20rpx 0;
}
.clerkWrap{
    width: 90%;
    margin: 0 auto;
    border-bottom: 2rpx solid #f9f9f9;
    overflow: hidden;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    flex-direction: row;
    flex-wrap: wrap;
}
.clerkWrap view {
    width: 33%;
    height: 60rpx;
    line-height: 60rpx;
    color: #989898;
    font-size: 24rpx;
    overflow: hidden;
}
.clerkWrap view.delBtn {
    width: 80rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin: 10rpx 0 0 0;
    border:2rpx solid #fb4b6b;
    color: #fb4b6b;
    border-radius: 10rpx;
    text-align: center;


}

.delBtn {
}
