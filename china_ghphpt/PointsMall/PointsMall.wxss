/* 导航样式*/

/* 导航样式*/

.swiper-tab {
  width: 100%;
  height: 80rpx;
  background: #fff;
  padding-bottom:20rpx;
}

swiper {
  width: 100%;
  height: 100%;
}

swiper-item {
  font-size: 28rpx;
  color: #989898;
  text-align: center;
  line-height: 80rpx;
}

swiper-item.active {
  color: #fb4b6b;
}

swiper-item.active::before {
  content: " ";
  width: 20rpx;
  height: 4rpx;
  background: #fb4b6b;
  position: absolute;
  bottom: 10rpx;
  left: 50%;
  margin-left: -10rpx;
}
/*内容*/

.scoll-h {
  height: 100%;
  width: 98%;
margin: 0px auto;
padding-bottom:150px;
}
.scroll-y{
  width:680rpx;
  margin:0px 35rpx;
}
.item {
  width:320rpx;
  float:left;
  margin: 10rpx 10rpx 0px;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  background:#fff;
  border-radius:10rpx;
  box-shadow: 5px 2px 6px #f2f2f2;
}

.item .img{
  width:100%; 
  overflow: hidden;
}
.item .img image{
  width:320rpx;
  height:320rpx;
  border-radius:10rpx 10rpx 0rpx 0rpx; 
}
.title{
  text-align:center;
  color:#666666;
  padding: 0px 10rpx;
  height: 50rpx;
  overflow: hidden;
}
.price{
 height:70rpx;
 line-height:70rpx;
 padding-bottom:20rpx;
 text-align:center;
 color:#fb4b6b;
 overflow: hidden;
}
.price i{
  font-size: 28rpx;
}

.jiage{
 width:38rpx;
 height:38rpx;
}



/* 商家列表 */
.businessListWrap{
  background: #f2f2f2;
  margin-top: 10rpx;
}
.businessList {
  width: 94%;
  height: 150rpx;
  border-radius: 10rpx;
  margin: 40rpx auto 60rpx;
  box-shadow: 10rpx 10rpx 12rpx 0rpx rgba(91, 21, 3, 0.2);
  position: relative;
  background: #fff url('data:image/png;base64,') no-repeat top 15rpx right 20rpx;
  background-size: 8rpx auto;
}

.businessList .businessLogo {
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  top: -20rpx;
  left: 0rpx;
  background: #ccc;
  border-radius: 10rpx;
}

.businessList .businessName {
  width: 480rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  top: -25rpx;
  left: 170rpx;
  overflow: hidden;
  font-size: 38rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #323642;
  text-align: left;
}

.businessList .businessAddr {
  width: 440rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  top: 40rpx;
  left: 170rpx;
  overflow: hidden;
  font-size: 24rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #323642;
  padding-left: 40rpx;
  background: url('data:image/png;base64,') no-repeat left center;
  background-size: 20rpx auto;
  text-align: left;
}

.businessList .businessTel {
  width: 440rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  top: 80rpx;
  left: 170rpx;
  font-size: 24rpx;
  color: #323642;
  padding-left: 40rpx;
  background: url('data:image/png;base64,') no-repeat left center;
  background-size: 20rpx auto;
  text-align: left;
}
.businessList .businessRange{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  top: 80rpx;
  right:30rpx;
  font-size: 22rpx;
  color: #323642;
  text-align: right;
}

.businessList .businessBtn {
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  position: absolute;
  bottom: -30rpx;
  right: 20rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
  background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
  border-radius: 10rpx;
}

.sellerListNo{
  display: none;
  width: 100%;
  height: 70vh;
  background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/noData.png') no-repeat center;
  background-size: 400rpx auto;
}
.nolist{
    display: block;
}
