<view class="container" id="mjltest">
    <view class="info">
        <button open-type="getUserInfo" bindgetuserinfo="onGotUserInfo" class="getUser" wx:if="{{!getUser}}">授权</button>
        <view class="logo">
            <button class="avatar-wrapper" open-type="chooseAvatar" bind:chooseavatar="onChooseAvatar">
                <image class="avatar" src="{{ member.avatar }}"></image>
            </button>
        </view>
        <view class="name">用户名：{{member.name}}</view>
        <view class="tel">手机号：{{phoneP}}</view>
        <view class="scan" wx:if="{{member.seller_title != null}}" bindtap="scan">扫一扫</view>
        <view class="businessName" wx:if="{{member.seller_title != null}}">店铺名：{{member.seller_title}}</view>
    </view>
    <view class="module coupon">
        <view class="title">我的卡包</view>
        <view class="con">
            <view>
                <navigator url="../mineCouponList/mineCouponList?type=2" hover-class="none">
                    <image class="yihexiao" src="data:image/png;base64,"></image>
                    已核销
                    <b>{{coupon_card_2_count}}张</b>
                </navigator>
            </view>
            <view>
                <navigator url="../mineCouponList/mineCouponList?type=2" hover-class="none">
                    <image class="yiyouhui" src="data:image/png;base64,"></image>
                    已优惠
                    <b>{{coupon_card_2_sum}}元</b>
                </navigator>
            </view>
            <view>
                <navigator url="../mineCouponList/mineCouponList?type=1" hover-class="none">
                    <image class="keshiyong" src="data:image/png;base64,"></image>
                    可使用
                    <b>{{coupon_card_1_count}}张</b>
                </navigator>
            </view>
        </view>
    </view>
    <view class="module integral">
        <view class="title">我的积分</view>
        <view class="con">
            <view bindtap="gotoDetial">
                <image style="width:32rpx" src="data:image/png;base64,"></image>
                <s>抽奖礼品</s>
            </view>

            <view>
                <navigator url="../signIn/signIn" hover-class="none">
                    <image class="qiandao" src="data:image/png;base64,"></image>
                    签到得积分
                </navigator>
            </view>
            <view>
                <navigator url="../Details/Details" hover-class="none">
                    <image class="mingxi" src="data:image/png;base64,iVBORw0KGgoAAAANSU "></image>
                    积分明细
                </navigator>
            </view>
            <view>
                <navigator url="../mineIntegralCL/mineIntegralCL" hover-class="none">
                    <image class="lipin" src="data:image/png;base64,"></image>
                    积分礼品
                </navigator>
            </view>
        </view>
    </view>
    <view class="module tool">
        <view class="title">常用工具</view>
        <view class="con">
            <view>
                <navigator url="../labour/components/index/index" hover-class="none" open-type='redirect'>
                    <image class='youhuiquan' src="data:image/png;base64,"></image>
                    劳动就业
                </navigator>
            </view>
            <view>
                <navigator url="../feedback/feedback" hover-class="none">
                    <image class='yijian' src="data:image/png;base64,"></image>
                    意见反馈
                </navigator>
            </view>
            <view>
                <navigator url="../messageList/messageList" hover-class="none">
                    <image class='xiaoxi' src="data:image/png;base64,"></image>
                    消息通知
                </navigator>
            </view>
            <view>
                <navigator url="../changePassWord/changePassWord" hover-class="none">
                    <image class='shezhi' src="data:image/png;base64,"></image>
                    个人设置
                </navigator>
            </view>
        </view>
    </view>

    <view class="module tool">
        <view class="title">特色功能</view>
        <view class="con">
            <view>
                <!-- url="/china_ghphpt/PaymentDetails/PaymentDetails" -->
                <navigator bindtap="weikaifang"  hover-class="none" open-type='navigate'>
                    <image class='youhuiquan' src="data:image/svg+xml;base64,"></image>
                    在线缴费
                </navigator>
            </view>
            <view>
                <navigator url="/china_ghphpt/h5Map/map" hover-class="none" open-type='navigate'>
                    <image class='youhuiquan' src="data:image/svg+xml;base64,"></image>
                    户外驿站
                </navigator>
            </view>
        </view>
    </view>
    <view class="module admin" wx:if="{{member.sell_user_type == 1}}">
        <view class="title">管理员</view>
        <view class="con">
            <view>
                <navigator url="../clerkAdd/clerkAdd" hover-class="none">
                    <image src="data:image/png;base64,"></image>
                    店员添加
                </navigator>
            </view>
            <view>
                <navigator url="../writeOff/writeOff" hover-class="none">
                    <image src="data:image/png;base64,"></image>
                    核销记录
                </navigator>
            </view>
            <view>
                <navigator url="../clerkList/clerkList" hover-class="none">
                    <image src="data:image/png;base64,"></image>
                    店员列表
                </navigator>
            </view>
            <view>
                <navigator url="../exchangeRecord/exchangeRecord?seller_id={{member.seller_id}}" hover-class="none">
                    <image src="data:image/png;base64,"></image>
                    兑换记录
                </navigator>
            </view>
        </view>
    </view>
    <view class="module admin" wx:if="{{member.sell_user_type == 2}}">
        <view class="title">管理员</view>
        <view class="con" style="justify-content:flex-start">
            <view>
                <navigator url="../cardWriteOff/cardWriteOff" hover-class="none">
                    <image src="data:image/png;base64,"></image>
                    核销记录
                </navigator>
            </view>
            <view>
                <navigator url="../exchangeRecord/exchangeRecord?seller_id={{member.seller_id}}" hover-class="none">
                    <image src="data:image/png;base64,"></image>
                    兑换记录
                </navigator>
            </view>
        </view>
    </view>

    <!-- 广告位 -->
    <view class="swiperWrap" wx:if="{{banner.length>0}}">
        <swiper circular="true" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
            <block>
                <swiper-item wx:for="{{banner}}" wx:for-item="banner_item" wx:key="banner_key">
                    <image src="{{banner_item.img}}" class="slide-image" />
                </swiper-item>
            </block>
        </swiper>
    </view>
    <view class="version">版本号：{{version}}</view>
</view>

<view class="footer">
    <view class="" bindtap="goto_index">
        <image src="{{setting.index_img_before}}"></image>
        <!-- <image src="{{setting.index_img_after}}"></image> -->
        <text>首页</text>
    </view>
    <view class="" bindtap="goto_businessList">
        <image src="{{setting.sell_img_before}}"></image>
        <!-- <image src="{{setting.sell_img_after}}"></image> -->
        <text>商户中心</text>
    </view>
    <view class="" bindtap="goto_PointsMall">
        <image src="{{setting.integral_img_before}}"></image>
        <!-- <image src="{{setting.integral_img_after}}"></image> -->
        <text>积分商城</text>
    </view>
    <view class="" bindtap="goto_couponList">
        <image src="{{setting.card_img_before}}"></image>
        <!-- <image src="{{setting.card_img_after}}"></image> -->
        <text>卡券列表</text>
    </view>
    <view class="on">
        <!-- <image src="{{setting.user_img_before}}"></image> -->
        <image src="{{setting.user_img_after}}"></image>
        <text>个人中心</text>
    </view>
</view>
<view class="tip" wx:if="{{ishowTip}}">
    <view class="tip_con">
        <view class="tip_title">{{status}}</view>
        <!-- <view class="tipsStatus">{{status}}</view> -->
        <view class="tip_detail">
            <text>{{tipsAlert}}</text>
            <view class="successBtn" bindtap="goMine">确定</view>
        </view>
    </view>
</view>