.tip {
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.65);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}

.tip_con {
  width: 500rpx;
  height: 350rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  background: #fff;
  border-radius: 20rpx;
  overflow: hidden;
}

.tip_title {
  width: 100%;
  height: 80rpx;
  background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
  text-align: center;
  line-height: 80rpx;
  color: #fff;
}

.tip_title i {
  width: 100%;
  height: 80rpx;
  display: block;
  background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/tip2.png') no-repeat center;
  background-size: 50rpx;
}

.tip_detail {
  text-align: center;
  font-size: 24rpx;
  color: #333;
}

.tip_detail text {
  line-height: 40rpx;
  height: 40rpx;
  margin: 30rpx auto;
  padding: 20rpx;
  display: block;
}


.successBtn {
  width: 200rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
  background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
  border-radius: 10rpx;
  display: block;
  margin: 0rpx auto;
}