/* china_ghphpt/Details/Details.wxss */
page {
  background: #f2f2f2;
}

.container {
  background: #f2f2f2;
  padding-bottom: 120rpx;
  height: calc(100% - 120px);
}

.top {
  width: 100%;
  height: 100rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: wrap;
  background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/bj1.jpg') no-repeat;
  background-size:100%  100%;
}

.top picker {
  width: 25%;
  margin: 20rpx 30rpx;
  padding: 0 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% auto;
}

/*卡券中*/
.conter {
  padding: 0px 55rpx;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  margin: 25rpx auto;
}

.conter .conter-z {
  color: #989898;
  width: 33.3333%;
  float: left;
  text-align: center;
  font-size: 24rpx;
}

.conter .conter-z .conter-red {
  color: #f95863;
}

.couponListWrap {
  background: #f2f2f2;
  padding-top: 20rpx;
}

/* 卡券列表 */

.couponWrap {
  display: block;
  position: relative;
  overflow: hidden;
  border-bottom: 1px solid #dce6f2;
  height: 90rpx;
  background: #fff;
  padding: 10px 55rpx;
  font-size: 24rpx;
  color: #989898;
}

.couponWrapleft {  
  float:left;
  line-height:40rpx;
}
.couponWrapright{
  float:right;
  text-align:right;
  line-height:40rpx;
}
.z-red{
  color: #f95863;
}








.couponL_type {
  font-size: 44rpx;
  line-height: 50rpx;
  font-weight: bold;
  color: #666;
  padding: 20rpx 20rpx 10rpx 20rpx;
}

.couponL_title {
  color: #666;
  font-size: 24rpx;
  padding: 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 30rpx;
  line-height: 30rpx;
  width: 90%;
}

.couponR_time {
  color: #666;
  font-size: 18rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding: 30rpx 20rpx 0rpx 20rpx;
  width: 330rpx;
}

.getBtn {
  text-align: center;
  color: #fff;
  background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
  border-radius: 10rpx;
  position: absolute;
  bottom: 25rpx;
  right: 20rpx;
  width: 110rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
}

.couponListNo {
  display: none;
  width: 100%;
  height: 70vh;
  background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/noData.png') no-repeat center;
  background-size: 400rpx auto;
}

.active {
  display: block;
}.noactive {
  display: none;
}