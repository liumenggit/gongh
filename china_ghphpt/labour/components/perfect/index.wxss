page{
    width: 100%;
    height: 100%;
}
.container{
    width: 100%;
    height: 100%;
    position: relative;
}
.detial_bg{
    position: absolute;
    left: 0;
    top: 0;
}
.content{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    padding-top:100rpx ;
    box-sizing: border-box;
    overflow-y: auto;
    padding-bottom: 50rpx;
}
.list{
    width: 90%;
    margin: 0 auto;
    background: #ffffff;
    border-radius: 15rpx;
    overflow: hidden;
}
.list_header{
    width: 100%;
    height: 80rpx;
    background: #FAE1E3;
    font-size: 28rpx;
    padding-left: 30rpx;
    line-height: 80rpx;
    box-sizing: border-box;

}
.column{
    height: 80rpx;
    line-height: 80rpx;
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid#f1f1f1;

}
.column_key{
    float: left;
    font-size: 30rpx;
    width: 130rpx;
    text-align: right    
}
.column_val{
    display: block;
    float: left;
    font-size: 30rpx;
    margin-left: 20rpx;
    width: 300rpx
}
.briPicker{
    margin-left: 20rpx;
    

}
.picker{
    font-size: 30rpx;
}
.sendBtn{
    width: 70%;
    margin: 0 auto;
    border-radius: 20rpx;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    background: #FB4B6B;
    color: #fff;
    font-size: 28rpx;
    margin-top: 100rpx;
}
.sexBox_{
    /* width: 200rpx;
    height: 40rpx; */
    margin-left: 50rpx;
}
.sex_{
    /* width: 46%; */
    float: left;
    margin-right: 2rpx;
    text-align: center;
    border: 1rpx solid#FB4B6B;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    padding:5rpx 20rpx ;
    color:#FB4B6B ;
}
.active{
  background: #FB4B6B ;
  color: #fff;
}
.widthLong{
  width: 187rpx
}