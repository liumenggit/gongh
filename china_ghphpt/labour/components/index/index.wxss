page{
    width: 100%;
    height: 100%;
    background: #f1f1f1;
}
.box{
  height: 100%
}
.navBar{
    width: 100%;
    background: #ffffff;
}
.searchBox{
    width: 100%;
    height: 108rpx;
    padding-top: 28rpx;
    box-sizing: border-box;
    position: relative;
}
.searchIpt{
    width: 572rpx;
    height: 60rpx;
    border-radius: 20rpx;
    margin-left: 50rpx;
    border:1rpx solid#f1f1f1;
    padding-left: 10rpx;
    box-sizing: border-box;
}
.searhBtn{
    position: absolute;
    right: 32rpx;
    top: 32rpx;
    width: 46rpx;
    height: 46rpx;
}
.navTab{
    width: 100%;
    height: 80rpx;
    background: #ffffff;
    overflow: hidden;
}
.tabList{
    padding: 10rpx 20rpx;
    float: left;
    position: relative;
}
.container_list{
    width: 100%;
    background: #f1f1f1;
    padding-bottom: 150rpx;
    overflow-y: auto;
    height:calc(100% - 200rpx);
    box-sizing: border-box;
}
.list{
    width: 90%;
    height:225rpx;
    margin: 20rpx auto;
    background: #ffffff;
    border-radius: 20rpx;
    padding: 20rpx;
    box-sizing: border-box;
}
.list_title{
    float: left;
    font: 32rpx;
    font-weight: bold;
}
.clear_{
    overflow: hidden;
}
.list_money{
    float: right;
    color: #FB4B6B;
}
.ruls_list{
    font-size: 24rpx;
    color: #c1c1c1;
    float: left;
    margin-top:20rpx
}
.list_from{
    margin-top: 20rpx;
}
.line{
    width: 1rpx;
    border: 1rpx solid#f1f1f1;
    float: left;
    height: 20rpx;
    margin: 25rpx 26rpx;
}
.list_from_img{
    width: 30rpx;
    height: 30rpx;
    float: left;

}
.list_from_text{
    float: left;
    margin-left: 10rpx;
}
.list_from_status{
    float: right;
    color: #c1c1c1;
    font-size: 24rpx;
}
.noData{
  height: 500rpx;
  width: 60%;
  background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/noData.png') no-repeat center;
  background-size: 100% 100%;
  margin: 0 auto;
  margin-top: 150rpx;
}
.moreIcon{
  position: absolute;
  right: 0;
  bottom: 20rpx;
  width: 20rpx;
  height: 20rpx;
}
.moreIcon image{
  width: 100%;
  height: 100%
}