page{
    width: 100%;
    height: 100%;
}
.container{
    width: 100%;
    height: 100%;
    position: relative;
}
.detial_bg{
    position: absolute;
    left: 0;
    top: 0;
}
.content{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    padding-top:100rpx ;
    padding-bottom:100rpx ;
    overflow-y:auto ;
    box-sizing: border-box;
}
.list{
    width: 90%;
    margin: 0 auto;
    background: #ffffff;
    border-radius: 15rpx;
    overflow: hidden;
}
.list_header{
    width: 100%;
    height: 80rpx;
    background: #FAE1E3;
    font-size: 28rpx;
    padding-left: 30rpx;
    line-height: 80rpx;
    box-sizing: border-box;

}
.column{
    display: flex;
    align-items: center;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid#f1f1f1;

}
.column_key{
    float: left;
    font-size: 30rpx;
    width: 130rpx;
    text-align: right;
}
.column_val{
    display: block;
    float: left;
    font-size: 30rpx;
    margin-left: 20rpx;
}
.describe{
    font-size: 30rpx;
}

.describeTxt{
width: 135rpx;
  text-align: right;
}
.describe_input{
    margin-top: 20rpx;
    padding: 20rpx;
}
.sendBtn{
    width: 70%;
    margin: 0 auto;
    border-radius: 20rpx;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    background: #FB4B6B;
    color: #fff;
    font-size: 28rpx;
    margin-top: 100rpx;
}
.briPicker{
    margin-left: 20rpx;

}
.picker{
    font-size: 30rpx;
}