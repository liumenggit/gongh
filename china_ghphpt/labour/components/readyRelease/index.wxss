page{
    width: 100%;
    height: 100%;
}
.container{
    width: 100%;
    height: 100%;
    background: #f1f1f1;
    padding-top: 30rpx;
    box-sizing: border-box;
    padding-bottom: 150rpx;
}
.list{
    width: 90%;
    height:225rpx;
    margin: 20rpx auto;
    background: #ffffff;
    border-radius: 20rpx;
    padding: 20rpx;
    box-sizing: border-box;
}
.list_title{
    float: left;
    font: 32rpx;
    font-weight: bold;
}
.clear_{
    overflow: hidden;
}
.list_money{
    float: right;
    color: #FB4B6B;
}
.ruls_list{
    font-size: 24rpx;
    color: #c1c1c1;
    float: left;
    margin-top:20rpx
}
.list_from{
    margin-top: 20rpx;
}
.line{
    width: 1rpx;
    border: 1rpx solid#f1f1f1;
    float: left;
    height: 20rpx;
    margin: 25rpx 26rpx;
}
.list_from_img{
    width: 40rpx;
    height: 40rpx;
    float: left;

}
.list_from_text{
    float: left;
    margin-left: 10rpx;
}
.repectBtn{
    float: right;
    color: #c1c1c1;
    font-size: 24rpx;
    padding: 10rpx 30rpx;
    text-align: center;
    color: #fff;
    border-radius: 10rpx;
    background: #FB4B6B;
}
.noData{
height: 500rpx;
width: 60%;
background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/noData.png') no-repeat center;
background-size: 100% 100%;
margin: 0 auto;
margin-top: 100rpx;

}