/* china_ghphpt/businessSearch/businessSearch.wxss */

page,.container {
    background: #f2f2f2;
}

/* 搜索框 */

.searchWrap {
    width: 100%;
    height: 100rpx;
    background: #fff;
}

.searchInput {
    width: 600rpx;
    float: left;
    height: 64rpx;
    font-size: 28rpx;
    margin: 18rpx 0 auto 20rpx;
    background: #f2f2f2 url('data:image/png;base64,') no-repeat left 20rpx center;
    background-size: 26rpx auto;
    border-radius: 50rpx;
}

.searchInput input {
    width: 500rpx;
    margin: 10rpx 0 0 60rpx;
    color: #000;
}

.input_placeholder {
    color: #989898;
}

.searchBtn {
    width: 100rpx;
    float: right;
    line-height: 100rpx;
    font-size: 28rpx;
    color: #fb4b6b;
}

/* 商家列表 */
.businessListWrap{
    background: #f2f2f2;
    margin-top: 10rpx;
}
.businessList {
    width: 94%;
    height: 150rpx;
    border-radius: 10rpx;
    margin: 40rpx auto 60rpx;
    box-shadow: 10rpx 10rpx 12rpx 0rpx rgba(91, 21, 3, 0.2);
    position: relative;
    background: #fff url('data:image/png;base64,') no-repeat top 15rpx right 20rpx;
    background-size: 8rpx auto;
}

.businessList .businessLogo {
    width: 150rpx;
    height: 150rpx;
    position: absolute;
    top: -20rpx;
    left: 0rpx;
    background: #ccc;
    border-radius: 10rpx;
}

.businessList .businessName {
    width: 480rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: absolute;
    top: -25rpx;
    left: 170rpx;
    overflow: hidden;
    font-size: 38rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #323642;
    text-align: left;
}

.businessList .businessAddr {
    width: 440rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: absolute;
    top: 40rpx;
    left: 170rpx;
    overflow: hidden;
    font-size: 24rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #323642;
    padding-left: 40rpx;
    background: url('data:image/png;base64,') no-repeat left center;
    background-size: 20rpx auto;
    text-align: left;
}

.businessList .businessTel {
    width: 440rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: absolute;
    top: 80rpx;
    left: 170rpx;
    font-size: 24rpx;
    color: #323642;
    padding-left: 40rpx;
    background: url('data:image/png;base64,') no-repeat left center;
    background-size: 20rpx auto;
    text-align: left;
}
.businessList .businessRange{
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: absolute;
    top: 80rpx;
    right:30rpx;
    font-size: 22rpx;
    color: #323642;
    text-align: right;
}

.businessList .businessBtn {
    width: 140rpx;
    height: 50rpx;
    line-height: 50rpx;
    position: absolute;
    bottom: -30rpx;
    right: 20rpx;
    font-size: 24rpx;
    text-align: center;
    color: #fff;
    background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
    border-radius: 10rpx;
}

.sellerListNo{
    display: none;
    width: 100%;
    height: 70vh;
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/noData.png') no-repeat center;
    background-size: 400rpx auto;
}
.nolist{
    display: block;
}