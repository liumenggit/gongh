    padding: 20rpx;
}
.ba10{
    border-radius: 20rpx;
}

.btlr10{
    border-radius: 20rpx 20rpx 0px 0px;
}
.topbox {
    width: 100%;
    height: 90rpx;
    position: relative;
    z-index: 1;
    background-color: #fb4b6b;
    display: flex;
    align-items: center;
}


.topbox .dingwei {
    /* display: none; */
