    font-size: 28rpx;
    z-index: 2;
    padding-left: 30rpx;
}
.topbox .showtc {
    position: absolute;
    left: 0;
    top: 180rpx;
    background: rgba(255, 255, 255, 0.8);
    z-index: 3;
    width: 180rpx;
    padding: 20rpx;
}

.dw-item {
    height: 60rpx;
    line-height: 60rpx;
    border-bottom: dashed 1px #ddd;
    font-size: 28rpx;
}

