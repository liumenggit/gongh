}

swiper {
    width: 100%;
    height: 100%;
}

swiper-item {
    width: 100%;
    height: 300rpx;
    margin: 20rpx auto;
}

.swiperWrap {
    width: 100%;
    height: 350rpx;
}

.swiperWrap image {
    width: 94%;
    height: 320rpx;
    margin: 0 3%;
    border-radius: 20rpx;
}

.categoryList {
    width: 94%;
    height: auto;
    max-height: 320rpx;
    margin: 10rpx auto;
    background: #fff;
    border-radius: 20rpx;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    flex-direction: row;
    flex-wrap: wrap;
}

.categoryList view {
    width: 25%;
    height: 160rpx;
}

.categoryList image {
    width: 100rpx;
    height: 100rpx;
    margin: 10rpx auto 0;
    display: block;
}

.categoryList text {
    font-size: 24rpx;
    text-align: center;
    display: block;
    color: #989898;
    width: 100%;
    height: 30rpx;
    overflow: hidden;
}

/* 通知滚动 */

.noticeWrap {
    width: 94%;
    height: 70rpx;
    background: #fff;
    margin: 10rpx auto 0;
    border-radius: 20rpx;
    font-size: 24rpx;
}

.noticeWrap .notice {
    width: 80rpx;
    height: 70rpx;
    float: left;
    text-align: center;
    line-height: 70rpx;
    color: #323642;
    background: url('data:image/png;base64,') no-repeat left 20rpx center;
    background-size: 40rpx auto;
    padding-left: 60rpx;
}

.noticeWrap swiper {
    width: calc(100% - 170rpx);
    float: right;
    height: 70rpx;
    background: url('data:image/png;base64,') no-repeat left center;
    background-size: 20rpx auto;
    padding-left: 30rpx;
}

.noticeWrap swiper swiper-item {
    margin: 0;
}

.noticeWrap view {
    display: block;
    height: 70rpx;
    line-height: 70rpx;
    color: #323642;
    width: 98%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* 平台信息 */

.infoWrap {
    width: 94%;
    height: 70rpx;
    background: #fff;
    margin: 4rpx auto 0;
    border-radius: 20rpx;
