.container {
    background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
}

.con {
    width: 690rpx;
    height: 1000rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: 100%;
    border-radius: 20rpx;
}

.back {
    color: #ff6c5f;
    position: absolute;
    top: 40rpx;
    left: 40rpx;
}

.seller {
