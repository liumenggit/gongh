/* china_ghphpt/h5Map/details/detial.wxss */
page{
  width: 100%;
  height: 100%;
}
.banner{
    height: 340rpx;
}
image{
    width: 100%;
}
.dcont{
  position: relative;
  border-bottom: 5px solid #f9f9f9 ;
  padding: 40rpx;
}
.dh{
    position: absolute;
    right: 10px;
    top: 50%;
    margin-top: -37rpx;
    height: 74rpx;
    width: 245rpx;
}
.js{
  padding: 40rpx;
}
.tit{
    font-size: 32rpx;
    color: #1296db;
    padding: 40rpx 0;
    font-weight: bold;
}
.cont{
  display: flex;
  flex-wrap: wrap;
}
.box{
    text-align: center;
    padding-top: 40rpx;
    width: 25%;
}
.iconsImg{
  width: 60rpx;
  height: 60rpx
}

.btn-certified {
    width: 100%;
    position: absolute;
    bottom: 100rpx;
    text-align: center;
}


.tip {
    width: 100vw;
    height: 100vh;
    background: rgba(0, 0, 0, 0.65);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    display: flex;
    flex-flow: row wrap;
    justify-content: center;
    align-items: center;
}

.tip_con {
    width: 500rpx;
    background: #fff;
    border-radius: 20rpx;
    overflow: hidden;
}

.tip_title {
    width: 100%;
    height: 80rpx;
    background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
}

.tipFail i {
    width: 100%;
    height: 80rpx;
    display: block;
    background: url('data:image/svg+xml;base64,') no-repeat center;
    background-size: 50rpx;
}

.tipSuccess i {
    width: 100%;
    height: 80rpx;
    display: block;
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/tip2.png') no-repeat center;
    background-size: 50rpx;
}

.tip_detail {
    text-align: center;
    font-size: 40rpx;
    color: #333;
}

.tip_detail text {
    line-height: 80rpx;
    height: 80rpx;
    margin: 40rpx auto;
    display: block;
}


.successBtn {
    width: 200rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 24rpx;
    text-align: center;
    color: #fff;
    background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
    border-radius: 10rpx;
    display: block;
    margin: 0rpx auto;
  }