/* china_ghphpt/h5Map/map.wxss */
page{
    width: 100%;
    height: 100%;
}
.mapContent{
    width: 100%;
    height: 100%;
}
.mapback{
    width: 90rpx;
    height: 90rpx;
    /* background:#4090e6; */
    position: fixed;
    right: 40rpx;
    bottom: 40rpx;
    border-radius: 50%;
    color: #fff;
    text-align: center;
    line-height: 90rpx;
    opacity: 0.8;
    /* border: 20rpx solid#ccc; */
}
.listBox{
    width: 100%;
    height: 100%;
}
.banner{
    height: 340rpx;
}
image{
    width: 100%;
}
.box{
    border-bottom:2rpx solid #ccc;
    position:relative;
    padding:30rpx 120rpx 30rpx 0
}
.list{
    padding-left: 68rpx;
    height: calc(100% - 340rpx);
    overflow-y: auto;

}
.num{
    background:#f5f5f5;
    width:40rpx;
    height:40rpx;
    line-height:40rpx;
    border-radius:50%;
    position:absolute;
    left:-50rpx;
    text-align:center;
    font-size:24rpx;
    color:#4090e6;
    top:50%;
    margin-top:-30rpx
}
.tit{
    font-size:32rpx;
    color:#000
}
.list_p{
    padding-top: 10px;
    color: #666;
    margin: 0;
}
.dh{
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -40rpx;
    width: 80rpx;
    height: 80rpx;
}
.iconbox{
    padding-top: 10rpx;
}
.iconImge{
    width: 30rpx;
    height: 30rpx;
    padding-right: 20rpx;
    padding-top: 10rpx;
}
.detialsIcon{
    border-radius: 50%;
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -40rpx;
    width: 100rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #1296db;
    /* background-color:#1296db */
}