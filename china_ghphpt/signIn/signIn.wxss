/* china_ghphpt/signIn/signIn.wxss */

page {
    background: #f9f9f9;
}

.container {
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/signInBg.jpg') no-repeat;
    background-size: 100% auto;
}

.info {
    width: 100%;
    height: 400rpx;
    overflow: hidden;
    color: #fff;
}

.logo {
    width: 50rpx;
    height: 50rpx;
    margin: 50rpx 20rpx 0rpx 50rpx;
    float: left;
    border-radius: 50rpx;
}

.name {
    margin-top: 50rpx;
    font-size: 28rpx;
    float: left;
}

.pointsDetails {
    background: #fff;
    color: #fb4b6b;
    width: 140rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin: 50rpx 50rpx 0rpx 50rpx;
    float: right;
    font-size: 24rpx;
    text-align: center;
    border-radius: 20rpx;
}

.point {
    width: 100%;
    display: block;
    clear: both;
    /* margin-top: 10rpx; */
    text-align: center;
    font-size: 60rpx;
    color: #ffe349;
}

.pointT {
    width: 100%;
    display: block;
    clear: both;
    margin-top: 10rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
}

.signInBtn {
    width: 400rpx;
    margin: 50rpx 175rpx;
    line-height: 100rpx;
    height: 100rpx;
    font-size: 34rpx;
    text-align: center;
    color: #fff;
    padding: 0;
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/signInBtn.png') no-repeat;
    background-size: 100% auto;
}

.signInTip {
    width: 94%;
    height: 260rpx;
    background: #fff;
    border-radius: 20rpx;
    margin: 0 auto;
    overflow: hidden;
}

.signInTTitle {
    width: 90%;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 5%;
    background: #fae1e3;
    color: #323642;
    font-size: 26rpx;
}

.signInTTitle i {
    color: #fb4b6b;
}

.signInTCon {
    width: 100%;
    height: 180rpx;
    position: relative;
}

.line {
    width: 86%;
    height: 4rpx;
    background: #ccc;
    position: absolute;
    top: 90rpx;
    left: 7%;
}

.day {
    color: #999;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    position: absolute;
    top: 100rpx;
    left: 10rpx;
    font-size: 20rpx;
    text-align: center;
}

.day:nth-child(2) {
    left: 10rpx;
}

.day:nth-child(3) {
    left: 110rpx;
}

.day:nth-child(4) {
    left: 210rpx;
}

.day:nth-child(5) {
    left: 310rpx;
}

.day:nth-child(6) {
    left: 410rpx;
}

.day:nth-child(7) {
    left: 510rpx;
}

.day:nth-child(8) {
    left: 610rpx;
}

.day i {
    display: block;
    width: 15rpx;
    height: 15rpx;
    background: #ccc;
    border-radius: 15rpx;
    position: absolute;
    top: -15rpx;
    left: 35rpx;
}

.day.on i {
    width: 30rpx;
    height: 30rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% auto;
    border-radius: 30rpx;
    position: absolute;
    top: -23rpx;
    left: 28rpx;
}

.day b {
    display: block;
    width: 70rpx;
    height: 50rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% auto;
    position: absolute;
    top: -70rpx;
    left: 6rpx;
    line-height: 34rpx;
    color: #fff;
}

.role {
    position: relative;
    width: 94%;
    height: 400rpx;
    background: #fff;
    border-radius: 20rpx;
    margin: 20rpx auto;
    overflow: hidden;
}

.role::after {
    content: " ";
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/roleAfter.png') no-repeat;
    background-size: 100% auto;
    width: 100%;
    height: 100rpx;
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
    z-index: 111;
}

.roleTitle {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background: #fae1e3;
    color: #323642;
    font-size: 24rpx;
}

.roleCon {
    width: 94%;
    margin: 0 auto;
    height: 240rpx;
    padding-bottom: 100rpx;
    overflow-y: scroll;
    font-size: 24rpx;
    color: #666;
    word-break: break-all;
    line-height: 40rpx;
}

.successTipW {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: rgba(0, 0, 0, 0.5);
}

.successTip {
    width: 500rpx;
    height: 340rpx;
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/successTip.png') no-repeat;
    background-size: 100% auto;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}
.successTip view{
    width: 90%;
    margin: 180rpx auto 0;
    text-align: center;
    color: #333333;
    font-size: 24rpx;
}
.successTip .successBtn {
    text-align: center;
    color: #fff;
    background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
    border-radius: 10rpx;
    position: absolute;
    bottom: 25rpx;
    left: 50%;
    margin: 0;
    margin-left: -55rpx;
    width: 110rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 20rpx;
}
