/* china_ghphpt/couponList/couponList.wxss */
page{
    background: #f2f2f2;
}
.container {
    background: #f2f2f2;
    padding-bottom: 120rpx;
    height: calc(100% - 120px);
}

.top {
    width: 100%;
    height: 100rpx;
    background: #f2f2f2;
    
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    flex-direction: row;
    flex-wrap: wrap;
}

.top picker{
    width: 25%;
    margin: 20rpx auto;
    padding: 0 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 24rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% auto;
}

.couponListWrap {
    background: #f2f2f2;
    border-top: 20rpx solid #fff;
    padding-top: 20rpx;
}

/* 卡券列表 */

.couponWrap {
    width: 94%;
    height: 200rpx;
    margin: 10rpx auto;
    display: block;
    position: relative;
    overflow: hidden;
}

.xianshi {
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/xianshin.png') no-repeat;
    background-size: 100% auto;
}

.changqi {
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/changqin.png') no-repeat;
    background-size: 100% auto;
}

.couponLeft {
    width: 23%;
    float: left;
}

.couponL_price {
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    line-height: 200rpx;
    overflow: hidden;
}

.couponL_price b {
    font-size: 58rpx;
    width: 100rpx;
    overflow: hidden;
    /* font-weight: bold; */
}

.couponRight {
    width: 76%;
    float: right;
}

.couponL_new {
    width: 100rpx;
    height: 70rpx;
    position: absolute;
    top: 0;
    right: 20rpx;
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/new.png') no-repeat;
    background-size: 100% auto;
}

.couponL_type {
    font-size: 44rpx;
    line-height: 50rpx;
    height: 45rpx;
    overflow: hidden;
    font-weight: bold;
    color: #666;
    padding: 20rpx 20rpx 10rpx 20rpx;
}

.couponL_title {
    color: #666;
    font-size: 24rpx;
    padding: 0 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 30rpx;
    line-height: 30rpx;
    width: 90%;
}

.couponR_time {
    color: #666;
    font-size:18rpx;
    height: 30rpx;
    line-height: 30rpx;
    padding: 30rpx 20rpx 0rpx 20rpx;
    width: 330rpx;
}

.getBtn {
    text-align: center;
    color: #fff;
    background: linear-gradient(70deg, #ff6c5f, #fb4b6b);
    border-radius: 10rpx;
    position: absolute;
    bottom: 25rpx;
    right: 20rpx;
    width: 110rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 20rpx;
}

.couponListNo{
    display: none;
    width: 100%;
    height: 70vh;
    background: url('https://wx.shuidiwang.cn/addons/china_ghphpt/resource/mobileImg/noData.png') no-repeat center;
    background-size: 400rpx auto;
}
.nolist{
    display: block;
}